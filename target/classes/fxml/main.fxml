<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.Button?>

<AnchorPane fx:id="panel" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.valdi.jmusicbot.MainController">
    <children>
        <MenuBar fx:id="menuBar" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <menus>
                <Menu fx:id="menuFile" mnemonicParsing="false" text="File"/>
                <Menu fx:id="menuEdit" mnemonicParsing="false" text="Edit"/>
                <Menu fx:id="menuHelp" mnemonicParsing="false" text="Help"/>
            </menus>
        </MenuBar>
        <Button fx:id="activateButton" layoutX="35.0" layoutY="250.0" mnemonicParsing="false" onAction="#onStartStop"
                prefHeight="25.0" prefWidth="100.0" text="Stop"/>
        <Button fx:id="closeButton" layoutX="160.0" layoutY="250.0" mnemonicParsing="false" onAction="#onClose"
                prefHeight="25.0" prefWidth="100.0" text="Close"/>
    </children>
</AnchorPane>
